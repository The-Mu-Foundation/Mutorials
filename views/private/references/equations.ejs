<!-- reference subpage -->

<!-- ARGUMENTS:
    equations - the equation list from backend -->

<%- include("../../partials/header") -%>
<%- include("../../partials/navigationPrivate") -%>

<h1 class="my-5 container">Common Equations</strong></h1>

<div class="container jumbotron mt-5">

    <% Object.keys(equations).forEach((equation) => { %>
        <div class="my-4 row">
            <div class="col-6">
                <h5><%= equation %></h5>
            </div>
            <div class="col-6">
                <p><%= equations[equation] %></p>
            </div>
        </div>
    <% }); %>

</div>

<div class="container">
    <a class="mb-5 btn btn-secondary" href="/references">Return to references page</a>
</div>

<%- include("../../partials/footer") -%>